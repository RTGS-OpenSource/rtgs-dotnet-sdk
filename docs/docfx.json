{
  "metadata": [
    {
      "src": [
        {
          "src": "..",
          "files": [
            "src/RTGS.DotNetSDK/RTGS.DotNetSDK.Publisher/**.csproj",
            "src/RTGS.DotNetSDK/RTGS.DotNetSDK.Subscriber/**.csproj"
          ]
        }
      ],
      "exclude": [
            "**/bin/**",
            "**/obj/**"
          ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "postProcessors": [ "ExtractSearchIndex" ],
    "globalMetadata": {
      "_appTitle": "RTGS Dotnet SDK documentation",
      "_appFooter": "<span>Copyright &copy; RTGS Global</span>"
    },
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "toc.yml",
          "index.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "statictoc",
      "rtgs-template"
    ]
  }
}